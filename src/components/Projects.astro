---
import ProjectItem from "./items/ProjectItem.astro";
import { project_dictionary } from "../i18n/translation";
import { getLangFromUrl } from "../i18n/utils";

const lang = getLangFromUrl(Astro.url);
---

{
  (lang === "es"
    ? project_dictionary.PROJECT_ES
    : project_dictionary.PROJECT_EN
  ).map((project) => <ProjectItem {...project} />)
}
